{
  "name": "pdf-you",
  "version": "0.0.1",
  "description": "PDF conversion library for Node.js",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=20.0.0"
  },
  "packageManager": "pnpm@10.26.2",
  "scripts": {
    "build": "tsup",
    "test": "vitest",
    "test:ci": "vitest run",
    "lint": "biome lint src",
    "lint:fix": "biome lint --write src",
    "format": "biome format --write src",
    "format:check": "biome format src",
    "check": "biome check src",
    "check:fix": "biome check --write src",
    "typecheck": "tsc --noEmit",
    "prepare": "husky || true && npm run build",
    "prepublishOnly": "pnpm test:ci && pnpm build",
    "changeset": "changeset",
    "version-packages": "changeset version",
    "release": "changeset publish"
  },
  "keywords": [
    "pdf",
    "conversion",
    "typescript",
    "node"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@biomejs/biome": "^2.3.10",
    "@changesets/changelog-github": "^0.5.2",
    "@changesets/cli": "^2.29.8",
    "@types/node": "^22.10.5",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsup": "^8.5.1",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ash-r1/pdf-you.git"
  },
  "bugs": {
    "url": "https://github.com/ash-r1/pdf-you/issues"
  },
  "homepage": "https://github.com/ash-r1/pdf-you",
  "lint-staged": {
    "src/**/*.ts": [
      "biome check --write --no-errors-on-unmatched"
    ]
  }
}
